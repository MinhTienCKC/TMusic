
@{
    ViewData["Title"] = "Đăng Nhập";

}
<link href="~/assets/css/dangnhap.css" rel="stylesheet" />
<div class="wrapper fadeInDown">
    <div id="formContent">
        <!-- Tabs Titles -->
        <h2 class="active"> Đăng Ký </h2>
        

        <!-- Icon -->
        <div class="fadeIn first">
            <img src="~/image/logo.png" id="icon" alt="User Icon" />
        </div>

        <!-- Login Form -->
        <form>
            <input type="text" id="email" class="fadeIn second" name="signup" placeholder="email">
            <input type="text" id="name" class="fadeIn third" name="signup" placeholder="name">
            <input type="text" id="password" class="fadeIn third" name="signup" placeholder="password">
            <input type="submit" class="fadeIn fourth" value="Đăng Ký">
        </form>

        <!-- Remind Passowrd -->
        <div id="formFooter">
           
            <a class="underlineHover" href="~/Admin/Account">Đăng Nhập</a>
            <a class="underlineHover" href="#">Quên Nhập Khẩu</a>
        </div>

    </div>
</div>
@section Scripts {
    @{ await Html.RenderPartialAsync("_ValidationScriptsPartial"); }
}
